  "files": [
    "dist"
  ],
  "scripts": {
    "watch": "tsup --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:tsc": "rimraf -rf dist esm lib && pnpm run build:esm && pnpm run build:cjs && pnpm run build-tailwind",
    "build:tsup": "tsup && pnpm run build-tailwind",
    "build-tailwind": "npx tailwindcss -o ./dist/styles.css --minify",
    "build:esm": "tsc --project tsconfig.build.json --outDir esm && tsc-alias --project tsconfig.build.json --outDir esm",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --outDir lib && tsc-alias --project tsconfig.build.json --outDir lib",
    "tspaths": "ts-node --project tsconfig.build.json -r tsconfig-paths/register esm/avatar/avatar.js",
    "dev": "pnpm run storybook",
    "storybook": "storybook dev -p 6060",
    "build-storybook": "storybook build",
    "_build": "rimraf -rf dist esm lib && pnpm build:tsup",
    "build:swc": "rimraf -rf dist esm lib && pnpm build:swc:esm && pnpm build:swc:cjs && pnpm run build-tailwind",
    "build:swc:esm": "pnpm generateType:esm && swc src -d esm --strip-leading-paths && tsc-alias --project tsconfig.build.json --outDir esm",
    "build:swc:cjs": "pnpm generateType:cjs && swc src -d lib --config module.type=commonjs --strip-leading-paths && tsc-alias --project tsconfig.build.json --outDir lib",
    "generateType:esm": "tsc --emitDeclarationOnly --project tsconfig.build.json --outDir esm",
    "generateType:cjs": "tsc --emitDeclarationOnly --project tsconfig.build.json --module commonjs --outDir lib"
  },
